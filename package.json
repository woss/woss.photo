{
  "name": "woss.photo",
  "version": "0.0.1",
  "author": {
    "email": "daniel@woss.io",
    "name": "woss"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev --port 1122",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint src --config eslint.config.js --cache --cache-strategy content --quiet",
    "lint:prettier": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "sentry": "bash ./sentryUpload.sh"
  },
  "type": "module",
  "dependencies": {
    "@steeze-ui/heroicons": "2.3.0",
    "@steeze-ui/svelte-icon": "1.5.0",
    "axios": "1.6.8",
    "blurhash": "2.0.5",
    "dayjs": "1.11.10",
    "exif-wasm": "0.1.1",
    "exifreader": "4.22.1",
    "ramda": "0.30.1",
    "@sentry/browser": "8.30.0",
    "@sentry/sveltekit": "8.30.0",
    "svelte-seo": "1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "9.0.0",
    "@playwright/test": "1.43.1",
    "@sinclair/typebox": "0.32.20",
    "@sveltejs/adapter-static": "3.0.1",
    "@sentry/cli": "2.30.2",
    "@sentry/types": "8.4.0",
    "@sveltejs/kit": "2.5.5",
    "@sveltejs/vite-plugin-svelte": "3.1.0",
    "@types/node": "20.12.7",
    "@types/ramda": "0.29.12",
    "autoprefixer": "10.4.19",
    "daisyui": "4.10.1",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-svelte": "2.37.0",
    "globals": "15.0.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "pretty-quick": "4.0.0",
    "simple-git-hooks": "2.11.1",
    "svelte": "4.2.14",
    "svelte-check": "3.6.9",
    "svelte-preprocess": "5.1.3",
    "tailwindcss": "3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "typescript-eslint": "7.6.0",
    "vite": "5.2.8",
    "vite-plugin-iso-import": "1.1.0",
    "vite-plugin-top-level-await": "1.4.1",
    "vite-plugin-wasm": "3.3.0",
    "vitest": "1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint && pnpm pretty-quick --staged"
  },
  "packageManager": "pnpm@9.6.0"
}
